<ul class="nav nav-tabs border-bottom border-2" id="traderTabs" role="tablist">
  <li class="nav-item" role="presentation">
    <button class="nav-link active border border-2 border-bottom-0 bg-white text-dark rounded-top"
      id="approved-tab" data-bs-toggle="tab" data-bs-target="#approved" type="button" role="tab">
      Approved
    </button>
  </li>
  <li class="nav-item" role="presentation">
    <button class="nav-link border border-2 border-bottom-0 bg-white text-dark rounded-top"
      id="confirmed-tab" data-bs-toggle="tab" data-bs-target="#confirmed" type="button" role="tab">
      Confirmed
    </button>
  </li>
  <li class="nav-item" role="presentation">
    <button class="nav-link border border-2 border-bottom-0 bg-white text-dark rounded-top"
      id="pending-tab" data-bs-toggle="tab" data-bs-target="#pending" type="button" role="tab">
      Pending
    </button>
  </li>
  <li class="nav-item" role="presentation">
    <button class="nav-link border border-2 border-bottom-0 bg-white text-dark rounded-top"
      id="rejected-tab" data-bs-toggle="tab" data-bs-target="#rejected" type="button" role="tab">
      Rejected
    </button>
  </li>
  <li class="nav-item" role="presentation">
    <button class="nav-link border border-2 border-bottom-0 bg-white text-dark rounded-top"
      id="banned-tab" data-bs-toggle="tab" data-bs-target="#banned" type="button" role="tab">
      Banned
    </button>
  </li>
  <li class="nav-item" role="presentation">
    <button class="nav-link border border-2 border-bottom-0 bg-white text-dark rounded-top"
      id="all-tab" data-bs-toggle="tab" data-bs-target="#all" type="button" role="tab">
      All
    </button>
  </li>
</ul>

<div class="tab-content border border-2 border-top-0 rounded-bottom p-3" id="myTabContent">
  <div class="tab-pane fade show active" id="approved" role="tabpanel">
    <%= render 'approved_traders' %>
  </div>
  <div class="tab-pane fade" id="confirmed" role="tabpanel">
    <%= render 'confirmed_traders' %>
  </div>
  <div class="tab-pane fade" id="pending" role="tabpanel">
    <%= render 'pending_traders' %>
  </div>
  <div class="tab-pane fade" id="rejected" role="tabpanel">
    <%= render 'rejected_traders' %>
  </div>
  <div class="tab-pane fade" id="banned" role="tabpanel">
    <%= render 'banned_traders' %>
  </div>
  <div class="tab-pane fade" id="all" role="tabpanel">
    <%= render 'all_traders' %>
  </div>
</div>
